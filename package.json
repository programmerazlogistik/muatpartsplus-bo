{
  "name": "muattrans-shipper",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run build:packages && next dev --turbopack",
    "build": "npm run build:packages && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky",
    "zip": "node ./scripts/zip.js",
    "t": "node scripts/translate.js",
    "translate:eka": "node scripts/translate-eka.js",
    "scan:untranslated": "node scripts/scan-untranslated.js",
    "scan:quick": "node scripts/quick-scan.js",
    "scan:view": "node scripts/view-scan-results.js",
    "lint:fix": "next lint --fix .",
    "build:packages": "turbo run build",
    "lint:packages": "turbo run lint",
    "clean": "rimraf --glob '**/{.next,.turbo,node_modules,dist,*.tsbuildinfo}'",
    "clean:windows": "rimraf --glob '**/{.next,.turbo,node_modules,dist,*.tsbuildinfo}' --impl=windows",
    "clean:linux": "rm -rf $(find . -type d \\( -name 'node_modules' -o -name '.next' -o -name '.turbo' -o -name 'dist' \\)) && find . -name '*.tsbuildinfo' -delete"
  },
  "dependencies": {
    "@million/lint": "^0.0.39",
    "@muatmuat/hooks": "*",
    "@muatmuat/lib": "*",
    "@muatmuat/ui": "*",
    "@react-google-maps/api": "^2.20.6",
    "lucide-react": "^0.545.0",
    "next": "^15.5.2",
    "react": "^19.1.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.1",
    "react-inlinesvg": "^4.2.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@ast-grep/cli": "^0.38.7",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.32.0",
    "@muatmuat/config": "*",
    "@muatmuat/tailwind-config": "*",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "turbo": "^2.5.6"
  },
  "packageManager": "npm@11.5.2"
}
